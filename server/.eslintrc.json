{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "operator-linebreak": 0,
    "max-len": 0,
    "arrow-body-style": 0,
    "object-curly-newline": 0,
    "consistent-return": 0
  }
}
